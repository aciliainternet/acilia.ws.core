{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img src='data:image/svg+xml;base64,'/>
        <span class="sf-toolbar-value">Widestand</span>
    {% endset %}

    {% set text %}
        <div class="sf-toolbar-info-group">
            <div class="sf-toolbar-info-piece">
                <b>Domain</b>
                <span>{{ collector.domain.host }} ({{ collector.domain.locale }})</span>
            </div>
            <div class="sf-toolbar-info-piece">
                <b>Build</b>
                <span class="sf-toolbar-status sf-toolbar-status-{{ collector.build == 'dev' ? 'green' : '' }}">{{ collector.build }}</span>
            </div>
        </div>

        <div class="sf-toolbar-info-group">
        {% for component, version in collector.components %}
            <div class="sf-toolbar-info-piece">
                <b>WS::{{ component }}</b>
                <span>{{ version }}</span>
            </div>
        {% endfor %}
        </div>
    {% endset %}

    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { link: false }) }}
{% endblock %}
